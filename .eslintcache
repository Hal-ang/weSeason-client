angeUserinfo = (e) => {\n    if (e.target.name === \"nick-name\") {\n      setNickname(e.target.value);\n    } else if (e.target.name === \"password\") {\n      setPassword(e.target.value);\n    } else setDoublePassword(e.target.value);\n  };\n\n  const handleSubmitUserinfo = () => {\n    //유효성 통과했다고 가정\n    // 1. 입력 비밀번호, 비밀번호 재확인 값이 동일해야 함\n    // 1-1. 같지 않다면 또 모달창으로 알려줌\n    // 2. 빈 문자열이면 안됨\n    // 3. 서버로 고객 정보 전달 PATCH\n\n    if (isNicknameChecked && !isNickname) {\n      setError(\"닉네임을 입력해주세요.\");\n    } else if (isPasswordChecked && (!isPasswordChecked || !isdoublePassword)) {\n      setError(\"비밀번호 또는 재확인 비밀번호가 일치하지 않습니다.\");\n    } else {\n      axios\n        .patch(\n          `${API_URL}/users`,\n          {\n            nickname: isNickname,\n            password: isPassword,\n          },\n          { headers: { authorization: `Bearer ${accessToken}` } }\n        )\n        .then(() => {\n          //성공시 마이페이지 리디렉션\n          handleChangeMypage();\n          // history.push(\"/Mypage\");\n        })\n        .catch((err) => {\n          //실패시 EditMypage 초기화면 리디렉션\n          console.log(err);\n        });\n    }\n  };\n\n  return (\n    <div className=\"EditMypage\">\n      <h1>Mypage</h1>\n      <div\n        className={\n          isPasswordChecked\n            ? \"EditMypage__contain--check\"\n            : \"EditMypage__contain--basic\"\n        }\n      >\n        <div className=\"EditMypage__ul\"></div>\n        <ul className=\"EditMypage__userinfo-contain\">\n          <li className=\"EditMypage__userinfo--item\">\n            <div className=\"EditMypage__item-title\">이름</div>\n            <div className=\"EditMypage_item-info\">{userinfo.name}</div>\n            <div className=\"EditMypage__userinfo__underline\"></div>\n          </li>\n\n          <li className=\"EditMypage__userinfo--item\">\n            <input\n              className=\"EditMypage__checkbox\"\n              type=\"checkbox\"\n              name=\"nick-name\"\n              value=\"nick-name\"\n              defaultChecked={isNicknameChecked}\n              onChange={handleChangeboxCheck}\n            ></input>\n\n            <div className=\"EditMypage__item-title nick-name\">닉네임</div>\n            {isNicknameChecked ? (\n              <input\n                className=\"EditMypage__input\"\n                type=\"text\"\n                name=\"nick-name\"\n                onChange={handleChangeUserinfo}\n              ></input>\n            ) : (\n              <div className=\"EditMypage_item-info\">{userinfo.nickname}</div>\n            )}\n\n            <div className=\"EditMypage__userinfo__underline\"></div>\n          </li>\n          <li className=\"EditMypage__userinfo--item\">\n            <div className=\"EditMypage__item-title\">email</div>\n            <div className=\"EditMypage_item-info\">{userinfo.email}</div>\n            <div className=\"EditMypage__userinfo__underline\"></div>\n          </li>\n          <li className=\"EditMypage__userinfo--item\">\n            <input\n              className=\"EditMypage__checkbox\"\n              type=\"checkbox\"\n              name=\"password\"\n              value=\"password\"\n              defaultChecked={isPasswordChecked}\n              onChange={handleChangeboxCheck}\n            ></input>\n            <div className=\"EditMypage__item-title password\">비밀번호</div>\n            {isPasswordChecked ? (\n              <input\n                className=\"EditMypage__input\"\n                type=\"password\"\n                name=\"password\"\n                onChange={handleChangeUserinfo}\n              ></input>\n            ) : (\n              <div className=\"EditMypage_item-info\">\n                ⚫️ ⚫️ ⚫️ ⚫️ ⚫️ ⚫️ ⚫️\n              </div>\n            )}\n\n            <div className=\"EditMypage__userinfo__underline\"></div>\n          </li>\n          {isPasswordChecked ? (\n            <li className=\"EditMypage__userinfo--item\">\n              <div className=\"EditMypage__item-title\">비밀번호 확인</div>\n              <input\n                className=\"EditMypage__input\"\n                type=\"password\"\n                name=\"password-double-check\"\n                onChange={handleChangeUserinfo}\n              ></input>\n              <div className=\"EditMypage__userinfo__underline\"></div>\n            </li>\n          ) : null}\n        </ul>\n      </div>\n      <button\n        className=\"EditMypage__submit-btn\"\n        name=\"submit\"\n        onClick={handleSubmitUserinfo}\n      >\n        submit\n      </button>\n      {isError ? (\n        <ErrorModal handleModalResponse={handleModalResponse} error={isError} />\n      ) : null}\n    </div>\n  );\n};\n\nexport default EditMypage;\n","/Users/hal-ang/Desktop/codestates/projects/weseason-client/src/Components/OriginMypage.js",[],"/Users/hal-ang/Desktop/codestates/projects/weseason-client/src/Component/MapModal.js",[],"/Users/hal-ang/Desktop/codestates/projects/weseason-client/src/Components/ErrorModal.js",[],"/Users/hal-ang/Desktop/codestates/projects/weseason-client/src/Components/WithdrawalModal.js",[],"/Users/hal-ang/Desktop/codestates/projects/weseason-client/src/Components/EditModal.js",["88"],"import React, { useState, useEffect } from \"react\";\nimport \"./EditModal.css\";\nimport axios from \"axios\";\nimport ErrorModal from \"./ErrorModal\";\nimport { API_URL } from \"../const\";\n\nconst EditModal = ({\n  handleModalResponse,\n  handleChangeMypage,\n  accessToken,\n}) => {\n  const [isCloseModal, setCloseModal] = useState(false);\n  const [isPassword, setPassword] = useState(\"\");\n  const [isFalsyPassword, setFalsyPassword] = useState(false);\n  const [isErrorMessage, setErrorMessage] = useState(\"\");\n\n  const handleEditModalClose = () => {\n    setCloseModal(true);\n    handleModalResponse();\n  };\n\n  const changePassword = (e) => {\n    setPassword(e.target.value);\n  };\n\n  const checkPassword = () => {\n    axios\n      .post(\n        `${API_URL}/auth/check`,\n        { password: isPassword },\n        { headers: { authorization: `Bearer ${accessToken}` } }\n      )\n      .then((data) => {\n        // 성공시 활성화된 수정 페이지로 이동, 렌더링\n        setCloseModal(true);\n        handleChangeMypage();\n      })\n      .catch((err) => {\n        // 실패시 오류 모달 노출\n        setFalsyPassword(true);\n        setCloseModal(true);\n        setErrorMessage(\"비밀번호를 재확인해주세요\");\n      });\n  };\n\n  return (\n    <>\n      {!isCloseModal ? (\n        <div className=\"EditModal__background\">\n          <div className=\"EditModal__contain\">\n            <p>본인확인을 위해 비밀번호를 입력해주세요.</p>\n            <input\n              className=\"EditModal__input-password\"\n              type=\"password\"\n              name=\"password\"\n              placeholder=\"비밀번호 입력\"\n              onChange={changePassword}\n            ></input>\n            <div className=\"EditModal__btn-contain\">\n              <button className=\"EditModal__btn\" onClick={checkPassword}>\n                yes\n              </button>\n              <button className=\"EditModal__btn\" onClick={handleEditModalClose}>\n                no\n              </button>\n            </div>\n          </div>\n        </div>\n      ) : null}\n      {isFalsyPassword ? (\n        <ErrorModal\n          handleModalResponse={handleModalResponse}\n          error={isErrorMessage}\n        />\n      ) : null}\n    </>\n  );\n};\n\nexport default EditModal;\n","/Users/hal-ang/Desktop/codestates/projects/weseason-client/src/Components/WithdrawalMember.js",[],"/Users/hal-ang/Desktop/codestates/projects/weseason-client/src/Component/Menu.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":16,"column":7,"nodeType":"95","messageId":"96","endLine":16,"endColumn":14},{"ruleId":"93","severity":1,"message":"97","line":1,"column":27,"nodeType":"95","messageId":"96","endLine":1,"endColumn":36},"no-native-reassign",["98"],